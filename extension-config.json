{
  "version": "2.2.8",
  "id": "@shopgate/shopify-cart",
  "steps": [
  ],
  "configuration": {
    "shopifyShopAlias": {
      "type": "bigApi",
      "destination" : "backend",
      "default": null,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.frontend.alias"
      }
    },
    "shopifyShopDomain": {
      "type": "bigApi",
      "destination" : "backend",
      "default": null,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.frontend.domain"
      }
    },
    "shopifyAccessToken": {
      "type": "bigApi",
      "destination" : "backend",
      "default": null,
      "params": {
        "method":  "GET",
        "service": "config",
        "path": "/v1/shop/%(shopId)s/checkout_web_checkout_shopify?parsed=true",
        "key": "value.backend.shopifyAccessToken"
      }
    },
    "enableCartCoupons" : {
      "type" : "static",
      "destination" : "backend",
      "params" : {
        "value" : false
      }
    }
  }
}
